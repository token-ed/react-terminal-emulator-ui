{
  "name": "react-terminal-emulator-ui",
  "version": "1.0.1",
  "description": "Terminal Emulator UI with React + Tailwind",
  "main": "src/index.tsx",
  "files": [
    "src/index.tsx",
    "src/components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/token-ed/react-terminal-emulator-ui"
  },
  "scripts": {
    "tw:build": "tailwindcss build -i ./src/styles/tailwind.css -o ./demo/output.css",
    "tw:docs:build": "tailwindcss build -i ./src/styles/tailwind.css -o ./docs/output.css",
    "tw:watch": "tailwindcss build -i ./src/styles/tailwind.css -o ./demo/output.css --watch",
    "start": "concurrently \"npm run tw:watch\" \"webpack-dev-server --mode development\"",
    "build": "rimraf docs && npm run tw:build && npm run tw:docs:build && webpack --color --mode production",
    "build-dev": "rimraf docs && webpack --color --mode development"
  },
  "keywords": [
    "terminal",
    "emulator",
    "react",
    "tailwind",
    "typescript",
    "bash",
    "shell",
    "linux",
    "ubuntu"
  ],
  "author": "Eduardo Fernandes",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "babel": "^6.23.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.2",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-webpack-plugin": "^5.6.0",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
